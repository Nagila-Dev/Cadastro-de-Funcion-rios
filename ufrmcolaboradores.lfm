      'left join rh_colab_cargo rhcargo on rhcargo.rc_id = rhcolab.rc_id'
      'left join rh_colab_foto rhf on rhf.rc_id = rhcolab.rc_id'
      'left join departamentos depart on depart.id_departamento = rhcolab.id_departamento'
